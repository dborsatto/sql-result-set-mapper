{
    "name": "dborsatto/sql-result-set-mapper",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "laminas/laminas-hydrator": "^4.0"
    },
    "require-dev": {
        "dborsatto/smart-enums": "^4.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.18",
        "vimeo/psalm": "^5.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
